# Example Environment Variables
# Copy this file to .env.local and fill in your actual values

# Development Environment
VITE_NODE_ENV=development

# Azure Cosmos DB Configuration
VITE_COSMOS_DB_ENDPOINT=https://localhost:8081
VITE_COSMOS_DB_KEY=your-cosmos-db-key-here
VITE_DATABASE_NAME=MappingAppDB

# LINZ Data Service API Key
# Get your free API key from: https://data.linz.govt.nz/
VITE_LINZ_API_KEY=your-linz-api-key-here

# MongoDB API (if using MongoDB syntax)
VITE_MONGO_URI=mongodb://localhost:10255

# API endpoints
VITE_API_BASE_URL=http://localhost:3001
